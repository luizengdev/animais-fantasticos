{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": 0,
    "quotes": 0,
    "comma-dangle": 0,
    "import/extensions": 0,
    "no-console": 0,
    "operator-linebreak": 0
  }
}
